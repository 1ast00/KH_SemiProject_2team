<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.WitnessMapper">
    <insert id="insertWitness" parameterType="dto.WitnessDTO">
        INSERT INTO witness
        (id, date, place, gender, age, etc, image)
        VALUES (#{id}, #{date}, #{place}, #{gender}, #{age}, #{etc}, #{image})
    </insert>

    <select id="selectWitnessList" resultType="dto.WitnessDTO">
        SELECT * FROM witness
    </select>
    
    <select id="selectWitnessById" parameterType="String" resultType="dto.WitnessDTO">
    SELECT * FROM witness WHERE id = #{id}
	</select>
    
</mapper>
