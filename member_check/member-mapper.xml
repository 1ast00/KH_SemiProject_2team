<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.MemberMapper">
	<resultMap type="dto.MemberDTO" id="member">
		<id column="member_serialNum" property="memberSerialNum"></id>
		<result column="admin_serialNum" property="adminSerialNum" />
		<result column="member_id" property="id" />
		<result column="member_pw" property="pw" />
		<result column="member_email" property="email" />
		<result column="member_name" property="name" />
		<result column="member_phone" property="phone" />
	</resultMap>
	
	<select id="selectById" resultType="int" parameterType="String">
		select count(*) from member_info where trim(member_id) = #{memberId}
	</select>
	
	
</mapper>